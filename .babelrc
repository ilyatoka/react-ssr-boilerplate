{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "env": {
    "server": {
      "plugins": [
        [
          "css-modules-transform",
          {
            "preprocessCss": "./sass.loader.js",
            "generateScopedName": "[name]__[local]__[hash:base64:5]",
            "extensions": [".scss", ".sass"]
          }
        ]
      ]
    }
  }
}
